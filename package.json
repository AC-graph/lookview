{
  "name": "lookview",
  "version": "2.0.0-alpha",
  "description": "提供更友好的数据可视化解决方案",
  "main": "dist/lookview.min.js",
  "scripts": {
    "start": "npm install && npm run dev",
    "dev": "rollup --config rollup.config.js --watch",
    "build": "cuf --delete dist && rollup --config rollup.config.js && uglifyjs ./dist/lookview.js -o ./dist/lookview.min.js && cuf --config ./cuf.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AC-graph/lookview.git"
  },
  "keywords": [
    "image2d",
    "graph"
  ],
  "author": "心叶",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AC-graph/lookview/issues"
  },
  "homepage": "https://github.com/AC-graph/lookview#readme",
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "cuf": "^1.2.0",
    "rollup": "^1.17.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.0.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "uglify-js": "^3.10.2"
  },
  "dependencies": {
    "@yelloxing/core.js": "^0.3.7",
    "image2d": "^1.8.11"
  }
}
